{
  "scripts": {
    "test": "nyc --reporter=text mocha --inspect --require babel-register --require source-map-support --recursive",
    "build": "babel src --out-dir js",
    "prepare": "npm run build",
    "watch": "babel --watch src --out-dir js",
    "coverage": "nyc report --reporter=text-lcov"
  },
  "main": "js/index.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "name": "@pubcore/knex-create-test-db",
  "version": "1.0.0",
  "description": "helper for unittest to create database table with knex",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pubcore/knex-create-test-db.git"
  },
  "keywords": [
    "knex",
    "unittest"
  ],
  "author": "pubcore",
  "bugs": {
    "url": "https://github.com/pubcore/knex-create-test-db/issues"
  },
  "homepage": "https://github.com/pubcore/knex-create-test-db#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "eslint-plugin-mocha": "^5.2.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2"
  },
  "dependencies": {
    "knex": "^0.15.2",
    "moment": "^2.22.2",
    "sqlite3": "^4.0.2"
  }
